
pipeline {
    agent any
    environment{
    CONTENT ='fast'
    }
    stages {
        stage('Checkout') {
            steps {
                git 'https://github.com/byoungjuchae/pytorch-to-onnx.git'
            }
        }
        stage('Build Docker Image') {
            steps {
             {
                   sh 'docker build -t fast -f /fastapi_onnx_docker_jenkins/app/Dockerfile .'
                }
            }
        
        }
    }
}
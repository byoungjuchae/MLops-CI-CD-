pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                git 'https://github.com/byoungjuchae/pytorch-to-onnx.git'
            }
        }
        stage('Build Docker Image') {
            steps {
                sh 'cd fastapi_onnx_docker_jenkins/app'
                script{
                    sh 'docker build -t fast .'
                    }
                }
            }
        
        // stage('Push Docker Image') {
        //     steps {
        //         script{
        //             sh 'docker run -d fast'
                
        //         }
        //     }
    }
}

        

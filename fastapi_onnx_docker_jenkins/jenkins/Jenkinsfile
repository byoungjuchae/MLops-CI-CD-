
pipeline {
    agent any
    environment{
    CONTENT ='fast'
    }
    stages {
        stage('Checkout') {
            steps {
                git 'https://github.com/byoungjuchae/pytorch-to-onnx.git'
            }
        }
        stage('Build Docker Image') {
            steps {
                dir('/fastapi_onnx_docker_jenkins/app'){
                    
                    app = docker.build("fast/jen:0.1v")
                }
            }
        
        // stage('Push Docker Image') {
        //     steps {
        //         script{
        //             sh 'docker run -d fast'
                
        //         }
        //     }
    }
    }
}